{
    "appId": "com.emsi.filemanager",
    "appName": "EMSI File Manager",
    "webDir": "dist",
    "server": {
        "androidScheme": "https",
        "allowNavigation": ["emsi-file.me"]
    },
    "plugins": {
        "CapacitorHttp": {
            "enabled": false
        },
        "SplashScreen": {
            "launchShowDuration": 2000,
            "launchAutoHide": true,
            "backgroundColor": "#0a0e1a",
            "showSpinner": false
        }
    }
}